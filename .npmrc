# Optimizaciones para reducir uso de memoria durante npm install
# Útil para entornos con recursos limitados como Jules

# Limitar operaciones concurrentes
maxsockets=5

# Usar cache agresivamente
prefer-offline=true

# Reducir logs para ahorrar memoria
loglevel=error

# Optimizar instalación
legacy-peer-deps=false
strict-peer-dependencies=false
